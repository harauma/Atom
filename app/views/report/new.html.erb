<%= form_with model: Report.new, url: 'new', local: true, class: 'ui form' do |form|%>
    <div class="form-group">
        <p>申請日</p>
        <%= form.datetime_field :appli_day %>
    </div>
    <div class="form-group">
    <p>報告日</p>
    <%= form.datetime_field :report_day %>
    </div>
    <div class="form-group">
    <p>企業コード</p>
    <%= form.text_field :job_code %>
    </div>
    <div class="form-group">
    <p>試験日時</p>
    <%= form.datetime_field :job_code %>
    </div>
    <p>受験場所</p>
    <%= form.text_field :job_code %>
    <p>受験エリア</p>
    <%= form.datetime_field :job_code %>
    
    <%= form.submit '登録',class:'btn-success.rounded-pills' %>
<% end%>